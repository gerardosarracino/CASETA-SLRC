                                            <t t-set="total_cuota_motocicleta" t-value="total_cuota_motocicleta +
                                            aforo_turno_motocicleta * motocicleta_tarifa_siva"/>
                                            <t t-esc="total_cuota_motocicleta" t-options='{"widget": "float", "precision": 2}' />
